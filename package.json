{
  "name": "scrollup-header",
  "version": "2.3.2",
  "description": "Fixed header for mobile devices. Shows up on scroll up",
  "main": "index.js",
  "module": "dist/scrollup-header.js",
  "scripts": {
    "start": "scriptkit",
    "dev": "scriptkit dev",
    "build": "scriptkit build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "scriptkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstandthird/scrollup-header.git"
  },
  "keywords": [
    "browser",
    "menu",
    "mobile"
  ],
  "author": "First+Third <code@firstandthird.com> (http://firstandthird.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstandthird/scrollup-header/issues"
  },
  "homepage": "https://github.com/firstandthird/scrollup-header#readme",
  "devDependencies": {
    "eslint-config-firstandthird": "4.4.0",
    "eslint-plugin-import": "2.18.0",
    "scriptkit": "3.0.0"
  },
  "scriptkit": {
    "serve": {
      "enabled": true
    },
    "test": {
      "enabled": false
    },
    "files": {
      "dist/scrollup-header.js": "index.js"
    },
    "scripts": {
      "babel": {
        "exclude": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "firstandthird",
    "env": {
      "browser": true
    }
  },
  "dependencies": {
    "domassist": "^2.2.0",
    "domodule": "^6.1.0",
    "scroll-bus": "^2.1.0",
    "tinybounce": "^1.2.0"
  }
}
